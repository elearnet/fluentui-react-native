{
  "name": "@elui-react-native/tester",
  "version": "0.170.49",
  "description": "A test app to test FluentUI React Native Components during development",
  "license": "MIT",
  "author": "Microsoft <fluentuinativeowners@microsoft.com>",
  "homepage": "https://github.com/microsoft/fluentui-react-native",
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "react-native": "src/index.ts",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib-commonjs/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "android": "rnx-cli run --platform android",
    "build": "fluentui-scripts build",
    "bundle": "rnx-cli bundle --dev false",
    "bundle:android": "rnx-cli bundle --dev false --platform android",
    "bundle:ios": "rnx-cli bundle --dev false --platform ios",
    "bundle:macos": "rnx-cli bundle --dev false --platform macos",
    "bundle:windows": "rnx-cli bundle --dev false --platform windows",
    "clean": "fluentui-scripts clean",
    "depcheck": "fluentui-scripts depcheck",
    "ios": "rnx-cli run --platform ios",
    "just": "fluentui-scripts",
    "lint": "fluentui-scripts eslint",
    "macos": "rnx-cli run --platform macos",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true",
    "start": "rnx-cli start --reset-cache",
    "windows": "react-native run-windows --arch x64 --sln windows/FluentTester.sln"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui-react-native.git",
    "directory": "apps/fluent-tester"
  },
  "dependencies": {
    "@elui-react-native/android-theme": "workspace:*",
    "@elui-react-native/apple-theme": "workspace:*",
    "@elui-react-native/avatar": "workspace:*",
    "@elui-react-native/badge": "workspace:*",
    "@elui-react-native/button": "workspace:*",
    "@elui-react-native/callout": "workspace:*",
    "@elui-react-native/chip": "workspace:*",
    "@elui-react-native/default-theme": "workspace:*",
    "@elui-react-native/divider": "workspace:*",
    "@elui-react-native/drawer": "workspace:*",
    "@elui-react-native/dropdown": "workspace:*",
    "@elui-react-native/e2e-testing": "workspace:*",
    "@elui-react-native/experimental-activity-indicator": "workspace:*",
    "@elui-react-native/experimental-appearance-additions": "workspace:*",
    "@elui-react-native/experimental-avatar": "workspace:*",
    "@elui-react-native/experimental-checkbox": "workspace:*",
    "@elui-react-native/experimental-expander": "workspace:*",
    "@elui-react-native/experimental-menu-button": "workspace:*",
    "@elui-react-native/experimental-native-date-picker": "workspace:*",
    "@elui-react-native/experimental-shadow": "workspace:*",
    "@elui-react-native/experimental-shimmer": "workspace:*",
    "@elui-react-native/framework": "workspace:*",
    "@elui-react-native/framework-base": "workspace:*",
    "@elui-react-native/icon": "workspace:*",
    "@elui-react-native/input": "workspace:*",
    "@elui-react-native/interactive-hooks": "workspace:*",
    "@elui-react-native/menu": "workspace:*",
    "@elui-react-native/notification": "workspace:*",
    "@elui-react-native/overflow": "workspace:*",
    "@elui-react-native/radio-group": "workspace:*",
    "@elui-react-native/separator": "workspace:*",
    "@elui-react-native/spinner": "workspace:*",
    "@elui-react-native/stack": "workspace:*",
    "@elui-react-native/switch": "workspace:*",
    "@elui-react-native/tablist": "workspace:*",
    "@elui-react-native/text": "workspace:*",
    "@elui-react-native/theme": "workspace:*",
    "@elui-react-native/theme-tokens": "workspace:*",
    "@elui-react-native/theme-types": "workspace:*",
    "@elui-react-native/themed-stylesheet": "workspace:*",
    "@elui-react-native/theming-utils": "workspace:*",
    "@elui-react-native/tooltip": "workspace:*",
    "@elui-react-native/vibrancy-view": "workspace:*",
    "@elui-react-native/win32-theme": "workspace:*",
    "@elui/react-native": "workspace:*",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@office-iss/react-native-win32": "^0.79.0",
    "@react-native-community/slider": "^4.5.7",
    "@react-native-menu/menu": "^0.7.3",
    "@react-native-picker/picker": "^2.7.0",
    "@types/node": "^22.0.0",
    "@types/react": "catalog:dev",
    "@warren-ms/react-native-icons": "^0.0.13",
    "react": "catalog:",
    "react-native": "catalog:",
    "react-native-macos": "catalog:",
    "react-native-nitro-modules": "catalog:",
    "react-native-svg": "^15.11.2",
    "react-native-windows": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "catalog:dev",
    "@babel/runtime": "catalog:dev",
    "@elui-react-native/eslint-config-rules": "workspace:*",
    "@elui-react-native/experimental-native-font-metrics": "workspace:*",
    "@elui-react-native/focus-zone": "workspace:*",
    "@elui-react-native/menu-button": "workspace:*",
    "@elui-react-native/scripts": "workspace:*",
    "@react-native-community/cli": "catalog:dev",
    "@react-native-community/cli-platform-android": "catalog:dev",
    "@react-native-community/cli-platform-ios": "catalog:dev",
    "@react-native/babel-preset": "catalog:dev",
    "@react-native/metro-babel-transformer": "^0.79.0",
    "@react-native/metro-config": "catalog:dev",
    "@rnx-kit/cli": "^0.18.14",
    "@rnx-kit/metro-config": "^2.1.0",
    "@rnx-kit/metro-resolver-symlinks": "^0.2.5",
    "@types/jasmine": "5.1.4",
    "@types/react-test-renderer": "catalog:dev",
    "@wdio/cli": "^8.40.0",
    "@wdio/globals": "^8.40.0",
    "@wdio/jasmine-framework": "^8.40.0",
    "flow-bin": "^0.113.0",
    "metro-config": "^0.81.3",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-test-app": "catalog:dev",
    "react-test-renderer": "catalog:dev",
    "webdriverio": "^8.40.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "kitType": "app",
    "bundle": [
      {
        "id": "main",
        "entryFile": "index.js",
        "assetsDest": "dist",
        "targets": [
          "android",
          "ios",
          "macos",
          "windows"
        ],
        "plugins": [
          [
            "@rnx-kit/metro-plugin-cyclic-dependencies-detector",
            {
              "throwOnError": true
            }
          ],
          [
            "@rnx-kit/metro-plugin-duplicates-checker",
            {
              "throwOnError": false
            }
          ]
        ],
        "platforms": {
          "android": {
            "bundleOutput": "dist/index.android.bundle",
            "sourcemapOutput": "dist/index.android.bundle.map",
            "assetsDest": "dist/res"
          },
          "ios": {
            "bundleOutput": "dist/index.ios.jsbundle",
            "sourcemapOutput": "dist/index.ios.jsbundle.map"
          },
          "macos": {
            "bundleOutput": "dist/index.macos.jsbundle",
            "sourcemapOutput": "dist/index.macos.jsbundle.map"
          },
          "windows": {
            "bundleOutput": "dist/index.windows.bundle",
            "sourcemapOutput": "dist/index.windows.bundle.map"
          }
        }
      }
    ],
    "alignDeps": {
      "presets": [
        "microsoft/react-native",
        "@elui-react-native/scripts/configs/align-deps-preset.cjs"
      ],
      "requirements": [
        "react-native@0.79"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core-android",
        "core-ios",
        "core-macos",
        "core-win32",
        "core-windows",
        "metro-config",
        "metro-react-native-babel-transformer",
        "react",
        "react-test-renderer",
        "svg",
        "test-app",
        "community/cli"
      ]
    }
  },
  "depcheck": {
    "ignoreMatches": [
      "@elui-react-native/experimental-expander"
    ]
  },
  "peerDependenciesMeta": {
    "@office-iss/react-native-win32": {
      "optional": true
    }
  }
}
