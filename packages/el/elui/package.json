{
  "name": "elui",
  "version": "0.1.0",
  "description": "for understanding how RN works under the hood",
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "react-native": "./src/index.tsx",
      "import": "./lib/index.js",
      "require": "./lib-commonjs/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "react-native": "src/index.tsx",
  "files": [
    "src",
    "lib",
    "lib-commonjs",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "build": "fluentui-scripts build",
    "clean": "fluentui-scripts clean",
    "depcheck": "fluentui-scripts depcheck",
    "lint": "fluentui-scripts eslint",
    "prettier": "fluentui-scripts prettier",
    "prettier-fix": "fluentui-scripts prettier --fix true"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elearnet/elui.git"
  },
  "author": "elearnet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elearnet/elui/issues"
  },
  "homepage": "https://github.com/elearnet/elui#readme",
  "devDependencies": {
    "@elui-react-native/eslint-config-rules": "workspace:*",
    "@elui-react-native/scripts": "workspace:*",
    "@react-native-community/cli": "^20.0.0",
    "@react-native/babel-preset": "0.79.1",
    "@types/react": "^19.0.3",
    "react": "19.0.3",
    "react-native": "^0.79.0",
    "react-native-macos": "^0.79.0",
    "react-native-windows": "^0.79.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react-native-macos": {
      "optional": true
    },
    "react-native-windows": {
      "optional": true
    }
  },
  "codegenConfig": {
    "name": "ELUISpec",
    "type": "all",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.elui"
    },
    "ios": {
      "components": {
        "FbrViewExample": {
          "className": "FbrViewExample1"
        },
        "SysIcon": {
          "className": "SysIcon"
        },
        "InvertedCorner": {
          "className": "InvertedCorner"
        }
      }
    }
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "turbo-module",
    "tools": [],
    "version": "0.55.1"
  },
  "dependencies": {
    "@elui-react-native/contextual-menu": "workspace:^",
    "react-native-markdown-display": "^7.0.2"
  }
}
